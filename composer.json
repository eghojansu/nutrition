{
	"name": "eghojansu/nutrition",
	"description": "Nutrition for fatfree framework",
	"homepage": "https://github.com/eghojansu/nutrition",
	"license": "MIT",
	"require": {
		"php": ">=5.4",
		"bcosca/fatfree": "3.5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.2"
	},
	"autoload": {
		"psr-4": {
			"Nutrition\\": "src/"
		},
		"files": ["Nutrition.php"]
	},
	"autoload-dev": {
		"psr-4": {
			"Nutrition\\Tests\\": "tests/unit/",
			"Nutrition\\Tests\\data\\": "tests/data/"
		}
	}
}